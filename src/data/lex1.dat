signature abs =
	a,b,c:type;
	t,u,v:(a -> a);
end


signature obj =
	s:type;
	tt,uu,vv:s;
	infix + : s -> s -> s;
	prefix - : s -> s;
	w=lambda x. tt+x:s->s;
	binder ALL = lambda P . P (w tt):(s -> s) -> s;
	z = ALL t.t+tt: s 
end

lexicon absobj ( abs ) : obj =
	a,b,c:=s;
	t:= lambda x.-x;
	u:= lambda x.ALL y.x+y ;
	v:=w
end
